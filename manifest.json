{
  "name": "Enhanced Wavefront UI",
  "version": "1.0",
  "description": "Enhancements to the Wavefront UI",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["src/add-edit-alert-button.js"],
      "matches": ["https://*.wavefront.com/charts*"]
    },
    {
      "js": ["src/add-view-alert-history-button.js"],
      "matches": ["https://*.wavefront.com/alerts/*"]
    }
  ]
}
